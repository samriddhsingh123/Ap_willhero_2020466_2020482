<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller2">
   <children>
      <AnchorPane fx:id="resume" prefHeight="800.0" prefWidth="1100.0" style="-fx-background-color: #0ba5e1;" styleClass="PauseBody" stylesheets="@background.css">
         <children>
            <Button fx:id="resu" layoutX="479.0" layoutY="225.0" mnemonicParsing="false" onAction="#resumeCurrent" prefHeight="69.0" prefWidth="183.0" style="-fx-background-color: #0be433;" text="Resume" textFill="WHITE" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="saveprog" layoutX="479.0" layoutY="322.0" mnemonicParsing="false" prefHeight="72.0" prefWidth="186.0" style="-fx-background-color: #0be433;" text="Save Progress" textFill="#fdfbfb" />
            <Button fx:id="mainmenu" layoutX="479.0" layoutY="427.0" mnemonicParsing="false" onAction="#exitToMainMenu" prefHeight="71.0" prefWidth="186.0" style="-fx-background-color: #0be433;" text="Exit To Main Menu" textFill="#fdfbfb">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
